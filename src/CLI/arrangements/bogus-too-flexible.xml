<cfg name="Bogus" mode="@[Mode]">

   <parameters>
      <add name="Mode" value="default" />
      <add name="Seed" value="1" />
      <add name="Size" value="5" />
      <add name="Provider" value="console" />
      
      <!-- Relational Provider Stuff -->      
      <add name="Server" value="localhost" />
      <add name="Database" value="Junk" />
      <add name="User" value="*" />
      <add name="Password" value="*" />
      
      <!-- file provider stuff -->
      <add name="File" value="temp.txt" />
      
      <!-- elasticsearch provider stuff -->
      <add name="Index" value="bogus" />
   </parameters>

   <connections>
      <add name="input" provider="bogus" seed="@[Seed]" />
      <add name="output"
           provider="@[Provider]"
           server="@[Server]"
           database="@[Database]"
           user="@[User]"
           password="@[Password]"
           file="@[File]"
           index="@[Index]"
           version="7.9.3" />
   </connections>

   <actions>
      <add type="run"
           before="true"
           after="false"
           connection="output" 
           command="CREATE DATABASE Junk;"
           error-mode="continue" />
   </actions>

   <entities>
      <add name="Contact" size="@[Size]">
         <fields>
            <add name="Identity" type="int" />
            <add name="FirstName" />
            <add name="LastName" />
            <add name="Email" />
            <add name="Phone" format="(###) ###-####" />
            <add name="Stars" type="byte" min="1" max="5" />
            <add name="Recent" alias="LastSeen" type="datetime" />
         </fields>
      </add>
   </entities>

</cfg>